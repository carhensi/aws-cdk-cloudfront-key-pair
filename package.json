{
  "name": "@carhensi/aws-cdk-cloudfront-key-pair",
  "description": "AWS CDK L3 construct for managing CloudFront trusted key group key pairs",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Carsten Hensiek",
    "email": "carsten@hensiek.com"
  },
  "homepage": "https://github.com/carhensi/aws-cdk-cloudfront-key-pair",
  "repository": {
    "type": "git",
    "url": "https://github.com/carhensi/aws-cdk-cloudfront-key-pair.git"
  },
  "license": "MIT",
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "aws-cdk-construct",
    "cloudfront",
    "key-pair"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:lambda": "npm run test --prefix src/create-key-pair",
    "test:all": "npm test && npm run test:lambda",
    "lint": "eslint lib src test --ext .ts",
    "lint:fix": "eslint lib src test --ext .ts --fix",
    "format": "prettier --write \"lib/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"lib/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.0.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.2.0",
    "typescript": "~5.7.0",
    "typescript-eslint": "^8.52.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.0.0"
  }
}
